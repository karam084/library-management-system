<p id="notice"><%= notice %></p>

<h1>Students</h1>
<table>
  <th><strong>Student Rights</strong></th>
  <th><strong>Student Name</strong></th>
  <th><strong>Student Email</strong></th>
  <th><strong>Student Password</strong></th>
  <th><strong>Student Password Confirmation</strong></th>
  <th><strong>Student Library</strong></th>
  <th><strong>Student Books</strong></th>
  <th><strong>Student Librarians</strong></th>
  <th><strong>Student Admins</strong></th>
  <th><strong>Student Libraries</strong></th>
  <th colspan="3"></th>

  <% @students.each do |student| %>
    <tr>
      <td><%= link_to 'Show', student %></td>
      <td><%= student.name %></td>
      <td><%= student.email %></td>
      <td><%= student.password %></td>
      <td><%= student.password_confirmation %></td>
      <td><%= student.library_id %></td>
      <td><%= student.book_id %></td>
      <td><%= student.librarian_id %></td>
      <td><%= student.admin_id %></td>
      <td><%= student.library_id %></td>
      <td><%= link_to 'Edit', edit_student_path(student) %></td>
      <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
  <% end %>
</table>

<br>

<%= link_to 'New Student', new_student_path %>
<br>
<%= link_to 'Back', :back %>
